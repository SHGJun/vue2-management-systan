{"ast":null,"code":"import { mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      menuData: [{\n        title: \"首页\",\n        id: \"1\"\n      }, {\n        title: \"用户管理\",\n        id: \"2\",\n        children: [{\n          title: \"用户权限\",\n          id: \"2-1\"\n        }, {\n          title: \"用户设置\",\n          id: \"2-2\"\n        }]\n      }]\n    };\n  },\n  computed: {\n    ...mapState({\n      isCollapse: state => state.navCollapse.isCollapse\n    })\n  },\n  methods: {\n    handleOpen() {},\n    handleClose() {}\n  }\n};","map":{"version":3,"names":["mapState","data","menuData","title","id","children","computed","isCollapse","state","navCollapse","methods","handleOpen","handleClose"],"sources":["src/layout/components/NavBarView.vue"],"sourcesContent":["<template>\n  <!-- isCollapse为真则为div增加类名为isCollapse的样式，否则无 -->\n  <div class=\"navbar\" :class=\"{ isCollapse }\">\n    <div class=\"title\">\n      <h1>\n        <!-- <img src=\"logo\" alt=\"\"> -->\n        <span>时光SHG</span>\n      </h1>\n    </div>\n    <el-menu\n      unique-opened\n      default-active=\"2\"\n      class=\"el-menu-vertical-demo\"\n      @open=\"handleOpen\"\n      @close=\"handleClose\"\n      background-color=\"#3f89fb\"\n      text-color=\"#fff\"\n      active-text-color=\"#ffd04b\"\n      :collapse=\"isCollapse\"\n      :collapse-transition=\"false\"\n    >\n      <div v-for=\"item in menuData\" :key=\"item.id\">\n        <el-submenu :index=\"item.id\" v-if=\"item.children\">\n          <template slot=\"title\">\n            <i class=\"el-icon-location\"></i>\n            <span>{{ item.title }}</span>\n          </template>\n          <el-menu-item\n            :index=\"child.id\"\n            v-for=\"child in item.children\"\n            :key=\"child.id\"\n            >{{ child.title }}</el-menu-item\n          >\n        </el-submenu>\n        <el-menu-item :index=\"item.id\" v-else>\n          <i class=\"el-icon-menu\"></i>\n          <span slot=\"title\">{{ item.title }}</span>\n        </el-menu-item>\n      </div>\n    </el-menu>\n  </div>\n</template>\n\n<script>\nimport { mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      menuData: [\n        {\n          title: \"首页\",\n          id: \"1\",\n        },\n        {\n          title: \"用户管理\",\n          id: \"2\",\n          children: [\n            {\n              title: \"用户权限\",\n              id: \"2-1\",\n            },\n            {\n              title: \"用户设置\",\n              id: \"2-2\",\n            },\n          ],\n        },\n      ],\n    };\n  },\n  computed: {\n    ...mapState({ isCollapse: (state) => state.navCollapse.isCollapse }),\n  },\n  methods: {\n    handleOpen() {},\n    handleClose() {},\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.navbar {\n  width: 220px;\n  height: 100%;\n  position: relative;\n  background-color: #3f89fb;\n  box-shadow: 6px 0 5px #ccc;\n  // 过渡\n  transition: all 0.3s;\n  .title {\n    text-align: center;\n    padding: 20px 0;\n    color: #fff;\n  }\n  .el-menu {\n    width: 100%;\n    border: none;\n  }\n  &.isCollapse {\n    width: 64px;\n  }\n  .isCollapse .el-submen8__title span{\n    display:none;\n  }\n}\n</style>"],"mappings":"AA4CA,SAAAA,QAAA;AACA;EACAC,KAAA;IACA;MACAC,QAAA,GACA;QACAC,KAAA;QACAC,EAAA;MACA,GACA;QACAD,KAAA;QACAC,EAAA;QACAC,QAAA,GACA;UACAF,KAAA;UACAC,EAAA;QACA,GACA;UACAD,KAAA;UACAC,EAAA;QACA;MAEA;IAEA;EACA;EACAE,QAAA;IACA,GAAAN,QAAA;MAAAO,UAAA,EAAAC,KAAA,IAAAA,KAAA,CAAAC,WAAA,CAAAF;IAAA;EACA;EACAG,OAAA;IACAC,WAAA;IACAC,YAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}