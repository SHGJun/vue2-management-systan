{"ast":null,"code":"import { mapState } from \"vuex\";\nexport default {\n  computed: {\n    ...mapState({\n      isCollapse: state => state.navCollapse.isCollapse\n    })\n  },\n  methods: {\n    handleOpen() {},\n    handleClose() {}\n  }\n};","map":{"version":3,"names":["mapState","computed","isCollapse","state","navCollapse","methods","handleOpen","handleClose"],"sources":["src/layout/components/NavBarView.vue"],"sourcesContent":["<template>\n  <!-- isCollapse为真则为div增加类名为isCollapse的样式，否则无 -->\n  <div class=\"navbar\" :class=\"{ isCollapse }\">\n    <div>\n      <span>时光SHG</span>\n    </div>\n    <el-menu\n      unique-opened\n      default-active=\"2\"\n      class=\"el-menu-vertical-demo\"\n      @open=\"handleOpen\"\n      @close=\"handleClose\"\n      background-color=\"#545c64\"\n      text-color=\"#fff\"\n      active-text-color=\"#ffd04b\"\n      :collapse=\"isCollapse\"\n      :collapse-transition=\"false\"\n    >\n      <el-submenu index=\"1\">\n        <template slot=\"title\">\n          <i class=\"el-icon-location\"></i>\n          <span>导航一</span>\n        </template>\n        <el-menu-item index=\"1-1\">选项1</el-menu-item>\n        <el-menu-item index=\"1-2\">选项2</el-menu-item>\n        <el-menu-item index=\"1-3\">选项3</el-menu-item>\n      </el-submenu>\n      <el-menu-item index=\"2\">\n        <i class=\"el-icon-menu\"></i>\n        <span slot=\"title\">导航二</span>\n      </el-menu-item>\n    </el-menu>\n  </div>\n</template>\n\n<script>\nimport { mapState } from \"vuex\";\nexport default {\n  computed: {\n    ...mapState({ isCollapse: (state) => state.navCollapse.isCollapse }),\n  },\n  methods: {\n    handleOpen() {},\n    handleClose() {},\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.navbar {\n  width: 220px;\n  height: 100%;\n  position: relative;\n  background-color: #00f2e00f;\n  box-shadow: 6px 0 5px #ccc;\n  // 过渡\n  transition: all .3s;\n  .el-menu {\n    width:100%;\n    border: none;\n  }\n  &.isCollapse {\n    width: 64px;\n  }\n}\n</style>"],"mappings":"AAoCA,SAAAA,QAAA;AACA;EACAC,QAAA;IACA,GAAAD,QAAA;MAAAE,UAAA,EAAAC,KAAA,IAAAA,KAAA,CAAAC,WAAA,CAAAF;IAAA;EACA;EACAG,OAAA;IACAC,WAAA;IACAC,YAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}